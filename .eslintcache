pedidoMenu.map(item => {\n                if (item.internalID === action.payload) {\n                    if (item.cant > 1) {\n                        item.cant = item.cant - 1\n                        return item\n                    }\n                } else {\n                    return item\n                }\n            })\n            return {\n                pedidoMenu: [...subs]\n            }\n        // break;\n        case 'SUM_PRODUCTS_MENU' :\n            const sum = state.pedidoMenu.map(item => {\n                if (item.internalID === action.payload) {\n                    item.cant = item.cant + 1\n                    return item\n                } else {\n                    return item\n                }\n            })\n            return {\n                pedidoMenu: [...sum]\n            }\n        // break;\n        case 'DELETE_PRODUCT_MENU':\n            const del = state.pedidoMenu.filter(item => item.internalID !== action.payload.product)\n            console.log('del', del)\n            console.log(action.payload)\n            return {\n                pedidoMenu: [...del]\n            }\n            // break;\n        case 'DISCHARD_PRODUCTS_FULL_MENU':\n            return {\n                pedidoMenu: []\n            }\n\n        default:\n            return state;\n    }\n}\n\nexport default PedidosMenu;","/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/ClientProfile.js",[],"/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/PedidosCarta.js",[],"/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/LastOrder.js",[],"/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/Publicidad.js",["372"],"// import ADD_ENTERPRISES from '../actions'\nimport SET_COUNT from '../actions'\n\nconst initialState = {\n    count: 0,\n    arrEnterprises: []\n};\n\nfunction Publicidad(state = initialState, action) {\n    switch (action.type) {\n        case 'ADD_ENTERPRISES':\n            return {\n                count: state.count,\n                arrEnterprises: action.payload\n            }\n        // break;\n        case 'SET_COUNT':\n            return {\n                count: action.payload,\n                arrEnterprises: state.arrEnterprises\n            }\n        // break;\n        default:\n            return state;\n    }\n}\n\nexport default Publicidad;","/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/SwitchMenu.js",[],"/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/DataProductSelected.js",[],"/home/alberto/WebstormProjects/ComandaApp/src/redux/reducers/Orders.js",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/Buttoninfo.jsx",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/Platosmenus.jsx",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/Emptymessage.jsx",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/Micomandamenu.jsx",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/Micomandacarta.jsx",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/homecomandapp/Buttonsubmit.jsx",[],"/home/alberto/WebstormProjects/ComandaApp/src/components/Carousel.jsx",["373"],"import React, {Fragment, useState, useEffect} from \"react\";\nimport Spinner from './Spinner';\nimport {\n    HTTP_PROTOCOL,\n    URL_MAIN,\n    USER_HEADERS,\n    PATH_API,\n} from '../data/connect_data_restaurantes';\nimport {dosDecim, urlImage} from \"../utils/utils\";\nimport Commandkeypad from './Commandkeypad';\nimport {connect} from \"react-redux\";\nimport \"../App.css\";\nimport axios from \"axios\";\n\nconst Carousel = ({\n                      datas,\n                      dataInicios,\n                      actualizaPropDataProductId,\n                      wordkey,\n                      products,\n                      token\n                  }) => {\n    const slide = {\n        column: {\n            display: 'flex',\n            flexDirection: 'column',\n            fontFamily: 'Dosis',\n       